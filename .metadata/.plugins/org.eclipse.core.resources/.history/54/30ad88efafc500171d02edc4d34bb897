package daa38;

import java.util.ArrayList;

public class VariablesContainer {
	
	ArrayList<Variable> mContainer;
	
	public VariablesContainer()
	{
		mContainer = new ArrayList<Variable>
	}
	
	public void AddVariable(Variable pVar)
	{
		mContainer.add(pVar);
	}
	
	public void DeleteVariable(Variable pVar)
	{
		mContainer.remove(pVar);
	}
}
