package daa38.CSP.LookBack;

import java.util.ArrayList;
import java.util.Collection;
import java.util.HashMap;
import java.util.Map;

import daa38.CSP.Auxiliary.StepFrame;
import daa38.CSP.Auxiliary.Variable;

public class GraphBasedBackjumping implements LookBack {

	Map<Variable, Collection<Variable>> mOtherDeadVars;
	
	public GraphBasedBackjumping()
	{
		mOtherDeadVars = new HashMap<Variable, Collection<Variable> >();
	}
	
	@Override
	public int jump(ArrayList<StepFrame> pSteps, int pIndex) {
		
		Variable lNowEnd = pSteps.get(pIndex).mVar;
		
		Collection<Variable> lOtherEnds = mOtherDeadVars.get(lNowEnd);
		
		Collection<Variable> lEnds;
		
		if (lOtherEnds == null)
		{
			lEnds = new ArrayList<Variable>();
		}
		else
		{
			lEnds = lOtherEnds
		}
		// TODO Auto-generated method stub
		return 0;
	}

}
