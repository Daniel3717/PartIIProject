package daa38.CSP.VariableOrdering;

import java.util.ArrayList;

import daa38.CSP.Auxiliary.Variable;

public interface VariableOrdering {
	
	Solver mSolver;
	
	protected VariableOrdering(Solver pSolver)
	{
		mSolver = pSolver;
	}
	
	public void order(ArrayList<Variable> pVars);//pSF will have only mAssVars assigned
}
