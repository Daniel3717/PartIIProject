public class VariablesRestrictions {
	public HashMap<Variable, Collection<Integer> > lVRs;
	
	public VariablesRestrictions()
	{
		lVRs = new HashMap<Variable, Collection<Integer> >();
	}
	
	public boolean enforceRestrictions()
	{
		
		return true;
	}
}
