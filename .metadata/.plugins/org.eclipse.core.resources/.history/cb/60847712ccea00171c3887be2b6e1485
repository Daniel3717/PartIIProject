package daa38.MapColouring.Auxiliary;

public class MatrixPosition {
	public int mRow,mCol;
	
	public MatrixPosition(int pRow, int pCol)
	{
		mRow = pRow;
		mCol = pCol;
	}
	
	public void up()
	{
		mRow--;
	}
	
	public void left()
	{
		mCol--;
	}
	
	public void down()
	{
		mRow++;
	}
	
	public void right()
	{
		mCol++;
	}
}
