package daa38.CSP.VariableOrdering;

import java.util.ArrayList;

import daa38.CSP.Auxiliary.Variable;
import daa38.CSP.Main.Solver;

public abstract class VariableOrdering {
	
	Solver mSolver;
	
	protected VariableOrdering(Solver pSolver)
	{
		mSolver = pSolver;
	}
	
	//pVars will be the variables to be ordered.
	//pVars will be modified, with the order of the variables being ascending
	public abstract Variable order(ArrayList<Variable> pVarsToOrder);
}
