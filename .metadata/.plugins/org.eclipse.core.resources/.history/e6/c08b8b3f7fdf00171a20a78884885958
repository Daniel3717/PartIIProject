package daa38.MapColoring;

import java.io.IOException;
import java.util.ArrayList;

public class MapColoring {

	public static int[][] generateMap(int pNrZones, int pSize)
	{
		return ForestMapGenerator.generateMap(pNrZones, pSize);
	}
	
	public static void main(String[] args) throws IOException {

		int[][] lMap = generateMap(11,5);
		
		for (int lRow = 0; lRow<lMap.length; lRow++)
		{
			for (int lCol = 0; lCol<lMap[lRow].length; lCol++)
			{
				System.out.print(lMap[lRow][lCol]);
			}
			System.out.println();
		}
	}
}
